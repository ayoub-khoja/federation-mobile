{
  "version": 2,
  "name": "federation-mobile-front",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/api/accounts/auth/(.*)",
      "dest": "https://federation-backend.onrender.com/api/accounts/auth/$1"
    },
    {
      "src": "/api/accounts/fcm/(.*)",
      "dest": "https://federation-backend.onrender.com/api/accounts/fcm/$1"
    },
    {
      "src": "/api/accounts/push/(.*)",
      "dest": "https://federation-backend.onrender.com/api/accounts/push/$1"
    },
    {
      "src": "/api/accounts/arbitres/profile/(.*)",
      "dest": "https://federation-backend.onrender.com/api/accounts/arbitres/profile/$1"
    },
    {
      "src": "/api/arbitres/(.*)",
      "dest": "https://federation-backend.onrender.com/api/arbitres/$1"
    },
    {
      "src": "/api/test/(.*)",
      "dest": "https://federation-backend.onrender.com/api/test/$1"
    },
    {
      "src": "/api/notifications/(.*)",
      "dest": "https://federation-backend.onrender.com/api/notifications/$1"
    },
    {
      "src": "/api/media/(.*)",
      "dest": "https://federation-backend.onrender.com/api/media/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
